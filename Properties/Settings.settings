      <Value Profile="(Default)">2222</Value>
    </Setting>
    <Setting Name="LDAP_GroupSearchBase" Type="System.String" Scope="User">
      <Value Profile="(Default)">ou=groups,dc=yourcompany,dc=com</Value>
    </Setting>
    <Setting Name="LDAP_UserFilter" Type="System.String" Scope="User">
      <Value Profile="(Default)">(objectClass=inetOrgPerson)</Value>
    </Setting>
    <Setting Name="LDAP_GroupFilter" Type="System.String" Scope="User">
      <Value Profile="(Default)">(objectClass=groupOfNames)</Value>
    </Setting>
  </Settings>
