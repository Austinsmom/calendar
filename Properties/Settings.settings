<?xml version='1.0' encoding='utf-8'?>
<SettingsFile xmlns="http://schemas.microsoft.com/VisualStudio/2004/01/settings" CurrentProfile="(Default)" GeneratedClassNamespace="RemObjects.Calendar.Properties" GeneratedClassName="Settings">
  <Profiles />
  <Settings>
    <Setting Name="LDAP_LoginDomain" Type="System.String" Scope="User">
      <Value Profile="(Default)">dn for a user that can SEARCH the ldap db</Value>
    </Setting>
    <Setting Name="LDAP_LdapServer" Type="System.String" Scope="User">
      <Value Profile="(Default)">ldap.yourserver.com</Value>
    </Setting>
    <Setting Name="LDAP_Password" Type="System.String" Scope="User">
      <Value Profile="(Default)">password for the LDAP_LoginDomain</Value>
    </Setting>
    <Setting Name="LDAP_CertHash" Type="System.String" Scope="User">
      <Value Profile="(Default)"></Value>
    </Setting>
    <Setting Name="LDAP_UserSearchBase" Type="System.String" Scope="User">
      <Value Profile="(Default)">ou=users,dc=yourcompany,dc=com</Value>
    </Setting>
